            },
            "1579849966": {
                "title": "Sales-channel doesn't have domain.",
                "message": "Sales-channel <a href=\"#/sw/sales/channel/detail/{salesChannelId}\">{salesChannelName}</a> need a domain for the releva.nz article export."
